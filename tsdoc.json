{
  "$schema": "https://developer.microsoft.com/json-schemas/tsdoc/v0/tsdoc.schema.json",
  "extends": ["@microsoft/tsdoc/tsdoc.json"],
  "noStandardTags": false,
  "tagDefinitions": [
    {
      "tagName": "@customRemarks",
      "syntaxKind": "block",
      "allowMultiple": false
    },
    {
      "tagName": "@privateRemarks",
      "syntaxKind": "block",
      "allowMultiple": false
    }
  ],
  "supportForTags": {
    "@alpha": true,
    "@beta": true,
    "@deprecated": true,
    "@eventProperty": true,
    "@example": true,
    "@experimental": true,
    "@inheritDoc": true,
    "@internal": true,
    "@label": true,
    "@link": true,
    "@override": true,
    "@packageDocumentation": true,
    "@param": true,
    "@privateRemarks": true,
    "@public": true,
    "@readonly": true,
    "@remarks": true,
    "@returns": true,
    "@sealed": true,
    "@see": true,
    "@since": true,
    "@throws": true,
    "@typeParam": true,
    "@virtual": true
  }
}